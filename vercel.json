{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://my-tms-projekt-backend-render.onrender.com/api/$1"
    },
    {
      "src": "/(assets/.*|favicon\\.ico|manifest\\.json|logo\\.png|.*\\.svg)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}